<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel ui:field="settingsMenuContainer">
		<g:HorizontalPanel>
			<g:Image></g:Image>
			<g:Label text="Beállítások"></g:Label>
			<g:StackLayoutPanel unit="PX" ui:field="settingsPanel">
				<g:stack>
					<g:header></g:header>
					<g:PushButton ui:field="newAnnouncement" text="Új bejelentés"></g:PushButton>
					<g:PushButton ui:field="editAnnouncement" text="Bejelentés szerkesztése"></g:PushButton>
				</g:stack>
			</g:StackLayoutPanel>
			
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 