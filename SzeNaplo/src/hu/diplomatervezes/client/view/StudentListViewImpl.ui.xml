<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="hu.diplomatervezes.client.view.StudentListViewImpl.RowStyle">
		.menuContener {
			background-color: #fff;
			width: 280px;
			height: auto;
	
		}
		
		.menuButtonContainer {
			height: 40px;
			width: 280px;

		}
		
		.selected {
			background-color: #ffffcc;
			color: fuchsia;
			padding: 11px!important;
			font-size: 16pt;
		}
		
		.unselected {
			background-color: #fff;

		}
	</ui:style>
	<g:VerticalPanel>
		<g:HorizontalPanel ui:field="menuButtonContainer" styleName="{style.menuButtonContainer}">
			<g:Label>Hello</g:Label>
		</g:HorizontalPanel>
		<g:VerticalPanel styleName="{style.menuContener}">	
			<g:StackLayoutPanel unit="PX" ui:field="studentListPanel"></g:StackLayoutPanel>
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 