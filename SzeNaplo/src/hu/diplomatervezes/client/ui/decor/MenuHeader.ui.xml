<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.label {
			padding-left: 5px;
			vertical-align: middle;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel>
			<g:Image url="/images/icons/osztaly.png"></g:Image>
			<g:Label styleName="{style.label}" ui:field="className"></g:Label>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 