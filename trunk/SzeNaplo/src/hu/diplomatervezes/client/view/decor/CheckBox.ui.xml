<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder">
	<ui:style type="hu.diplomatervezes.client.view.decor.CheckBox.CbStyle">
	.unChecked{
		
		width: 11px;
		height: 11px;
		
		background-color: (255, 255, 255, 0.05);
		border: 1px solid rgba(155, 155, 155, 0.57);
		border-radius: 1px 1px 1px 1px;
		display: block;
	}
	.unChecked:hover{
		width: 11px;
		height: 11px;
		
		background-color: (255, 255, 255, 0.05);
		border: 1px solid #B2B2B2;
		box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1) inset;
		border-radius: 1px 1px 1px 1px;
		display: block;
	}	
	.checked {
	
	}
	
	</ui:style>
	<div styleName="{style.unChecked}" ui:field="checkBox">
		
	</div>
</ui:UiBinder>