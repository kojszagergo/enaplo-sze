<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<table>
			<tr>
				<td>Vezetéknév</td>
				<td><g:TextBox ui:field="vezetekNev"></g:TextBox></td>
				
				<td>Keresztnév</td>
				<td><g:TextBox ui:field="keresztNev"></g:TextBox></td>
				
				<td>Született</td>
				<td><g:TextBox ui:field="szuletesiDatum"></g:TextBox></td>
				
				<td>Neme</td>
				<td><g:TextBox ui:field="nem"></g:TextBox></td>
				
				<td>Irányítószám</td>
				<td><g:TextBox ui:field="iranyitoSzam"></g:TextBox></td>
				
				<td>Város</td>
				<td><g:TextBox ui:field="varos"></g:TextBox></td>
				
				<td>Utca, házszám</td>
				<td><g:TextBox ui:field="utca"></g:TextBox></td>
				
				<td>Anyja neve</td>
				<td><g:TextBox ui:field="anyjaNeve"></g:TextBox></td>
							
			</tr>
		
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 