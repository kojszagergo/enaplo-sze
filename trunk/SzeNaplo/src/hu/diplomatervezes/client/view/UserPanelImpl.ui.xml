<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.container {
		
	}
	.userIcon {
		padding-left: 20px;
		padding-right: 5px;
	}
	
	</ui:style>
	<g:HorizontalPanel styleName="{style.container}">
		<g:Image url="/images/home.png" styleName="{style.userIcon}"></g:Image>
		<g:MenuBar autoOpen="false" animationEnabled="true" height="25px">
			<g:MenuItem ui:field="mainMenu">
				<g:MenuBar vertical="true">
					<g:MenuItem text="Beállítások" ui:field="settings"></g:MenuItem>
					<g:MenuItem text="Kijelentkezés" ui:field="logout"></g:MenuItem>
				</g:MenuBar>
			</g:MenuItem>
		</g:MenuBar>
	</g:HorizontalPanel>
</ui:UiBinder> 